     RowBox[{
      RowBox[{"g", " ", "\[Mu]B"}], " ", "|", "B", "|", "\[IndentingNewLine]", 
      RowBox[{"\[Implies]", 
       RowBox[{"|", "B", "|"}]}]}], " ", "=", " "}]}], TraditionalForm]],
